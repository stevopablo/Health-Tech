<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/anti_fleshwhite"
    tools:context=".Agendados">

    <ImageButton
        android:id="@+id/goToseeMore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_gravity="right"
        android:layout_marginBottom="12sp"
        android:background="@color/anti_fleshwhite"
        android:padding="12sp"
        android:rotation="40"
        android:src="@drawable/baseline_add_circle_outline_24" />

    <ImageButton
        android:id="@+id/CancelBtn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="7dp"

        android:layout_marginTop="62dp"
        android:src="@drawable/group" />

    <!--    navbar-->

    <ImageButton
        android:id="@+id/CancelBtn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="153dp"
        android:src="@drawable/group" />

    <ImageButton
        android:id="@+id/CancelBtn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="15dp"
        android:src="@drawable/group" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="61dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="0dp"
        android:layout_marginBottom="2dp"
        android:background="@color/white" />

    <ImageButton
        android:id="@+id/backTo"
        android:layout_width="70dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="3dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/baseline_home_24" />

    <ImageButton
        android:id="@+id/calendarButton"
        android:layout_width="79dp"
        android:layout_height="58dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="82dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="@color/white"

        android:src="@drawable/baseline_calendar_month_24" />

    <ImageButton
        android:id="@+id/userButton"
        android:layout_width="73dp"
        android:layout_height="63dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="@color/white"

        android:src="@drawable/baseline_account_circle_24" />

    <ImageButton
        android:id="@+id/settingsButton"
        android:layout_width="69dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="330dp"
        android:layout_marginTop="667dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/baseline_settings_24" />


    <ImageButton
        android:id="@+id/AddConsul"
        android:layout_width="79dp"
        android:layout_height="66dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="167dp"
        android:background="@color/white"
        android:layout_marginBottom="33dp"
        android:src="@drawable/baseline_add_circle_outline_24" />

</RelativeLayout>
